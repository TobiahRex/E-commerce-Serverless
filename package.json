  "scripts": {
    "analyze-bundle": "NODE_ENV=production babel-node ./tools/analyzeBundle.js",
    "deploy": "NODE_ENV=production npm-run-all prebuild build:bundle copy:assets s3:clean s3:deploy cf:invalidate",
    "deployTroubleshoot": "NODE_ENV=troubleshoot npm-run-all prebuild build:bundle copy:assets s3:cleanTroubleshoot s3:deployTroubleshoot cf:invalidate",
    "build:bundle": "babel-node ./tools/build.js",
    "cf:invalidate": "babel-node ./tools/cf_invalidate_script.js",
    "copy:assets": "npm-run-all copy:error copy:package copy:images copy:normalize copy:webflow-css copy:webflow-js",
    "copy:error": "copyfiles -f './src/containers/404/error/*' ./dist",
    "copy:package": "copyfiles 'package.json' ./dist/",
    "copy:images": "copyfiles -f './src/images/*' ./dist/images/",
    "copy:normalize": "copyfiles -f './src/webflow/nj2jp/css/normalize.css' ./dist/",
    "copy:webflow-css": "copyfiles -f './src/webflow/nj2jp/css/webflow.css' ./dist/",
    "copy:webflow-js": "copyfiles -f './src/containers/splash/assets/utils/webflow.js' ./dist/",
    "graphql": "babel-watch ./server/graphiql/server.js",
    "s3:clean": "aws s3 rm s3://nj2jp-react --recursive",
    "s3:deploy": "aws s3 sync dist/ s3://nj2jp-react",
    "s3:cleanTroubleshoot": "aws s3 rm s3://nj2jp-troubleshoot --recursive",
    "s3:deployTroubleshoot": "aws s3 sync dist/ s3://nj2jp-troubleshoot",
    "s3:copyConfig": "npm-run-all s3:serverlessProductionConfig s3:serverlessTroubleshootConfig s3:reactEnv",
    "s3:serverlessProductionConfig": "aws s3 cp serverless/config.production.yml s3://nj2jp-environment-files/serverless/",
    "s3:serverlessTroubleshootConfig": "aws s3 cp serverless/config.troubleshoot.yml s3://nj2jp-environment-files/serverless/",
    "s3:reactEnv": "aws s3 cp .env s3://nj2jp-environment-files/react/",
    "lambda:warmUp": "babel-node ./tools/warmUpLambdaScript.js",
    "lambda:deployTroubleshoot": "cd ./serverless && serverless deploy --stage troubleshoot -vvvv",
    "lambda:deployProduction": "cd ./serverless && serverless deploy --stage production -vvvv",
    "sls-deploy-troubleshoot": "NODE_ENV=troubleshoot npm-run-all lambda:deployTroubleshoot lambda:warmUp",
    "sls-deploy-production": "NODE_ENV=production npm-run-all lambda:deployProduction lambda:warmUp",
    "start": "NODE_ENV=development babel-watch ./server/server.js",
    "start-prod": "babel-node ./tools/distServer.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "rimraf ./dist && mkdir dist"
  },
  "author": "Tobiah Rex",
